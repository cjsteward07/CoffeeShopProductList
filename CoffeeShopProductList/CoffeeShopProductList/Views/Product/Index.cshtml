@model IEnumerable<CoffeeShopProductList.Models.ProductViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Coffee Shop Product List</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Details", new {  id= item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
